{
    "name": "hola_mundo_express",
    "version": "1.0.0",
    "description": "ejemplo hola mundo",
    "main": "app.js",
    "scripts": {
        "test": "tape test/*.js | tap-spec",
        "lint": "xo",
        "security-check": "nsp check",
        "coverage": "nyc npm test",
        "coverage:html": "nyc npm test && nyc report --reporter=html",
        "coveralls": "nyc npm test && nyc report --reporter=text-lcov | coveralls",
        "ci": "npm run lint && npm run coveralls",
        "dependencyCheck": "szero . --ci",
        "release": "standard-version -a",
        "openshift": "nodeshift --imageTag=12.x --strictSSL=false --tryServiceAccount=false --configLocation /openshift.local.config/master/admin.kubeconfig",
        "start": "node ."
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/eduardovc/hola_mundo_express.git"
    },
    "keywords": [
        "express",
        "node"
    ],
    "author": "Eduardo ventura",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/eduardovc/hola_mundo_express/issues"
    },
    "homepage": "https://github.com/eduardovc/hola_mundo_express#readme",
    "dependencies": {
        "express": "^4.17.1"
    }
}